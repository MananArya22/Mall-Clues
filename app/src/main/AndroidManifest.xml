        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.GoogleSignInActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.MallsListActivity" />
        <activity android:name=".ui.MallInformationActivity" />
        <activity android:name=".ui.StoresListActivity" />
        <activity android:name=".ui.StoreInformationActivity" />
        <!--<activity android:name=".GoogleSignInActivity" />-->
    </application>

</manifest>